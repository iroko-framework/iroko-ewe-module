@prefix iroko: <https://iroko-framework.github.io/iroko-ewe-module/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

iroko:accessLevel a owl:ObjectProperty ;
  rdfs:label "access level"@en ;
  rdfs:comment "Governance designation indicating the disclosure level for culturally sensitive fields."@en ;
  rdfs:range skos:Concept ;
  dcterms:isPartOf iroko:AccessLevelScheme .

iroko:AccessLevelScheme a skos:ConceptScheme ;
  skos:prefLabel "Access level scheme"@en ;
  skos:definition "Controlled values for disclosure and governance of culturally sensitive fields."@en .

iroko:open a skos:Concept ;
  skos:inScheme iroko:AccessLevelScheme ;
  skos:prefLabel "open"@en ;
  skos:definition "General botanical and bibliographic description."@en .

iroko:limited a skos:Concept ;
  skos:inScheme iroko:AccessLevelScheme ;
  skos:prefLabel "limited"@en ;
  skos:definition "High-level contextual description without procedural detail."@en .

iroko:restricted a skos:Concept ;
  skos:inScheme iroko:AccessLevelScheme ;
  skos:prefLabel "restricted"@en ;
  skos:definition "Culturally sensitive context; non-operational and not published beyond approved contexts."@en .
